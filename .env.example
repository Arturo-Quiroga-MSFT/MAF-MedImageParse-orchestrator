# Azure AI Foundry Configuration
AZURE_AI_PROJECT_ENDPOINT=https://your-project.services.ai.azure.com/api/projects/your-project-id
AZURE_SUBSCRIPTION_ID=your-subscription-id
AZURE_RESOURCE_GROUP=your-resource-group
MODEL_DEPLOYMENT_NAME=gpt-4o

# MedImageParse Configuration
MEDIMAGEPARSE_ENDPOINT=https://your-medimageparse-endpoint.inference.ml.azure.com
MEDIMAGEPARSE_API_KEY=your-api-key

# Azure Storage Configuration (for image staging)
AZURE_STORAGE_CONNECTION_STRING=your-storage-connection-string
AZURE_STORAGE_CONTAINER_NAME=medical-images

# Azure AI Search Configuration (for vector search)
AZURE_SEARCH_ENDPOINT=https://your-search-service.search.windows.net
AZURE_SEARCH_API_KEY=your-search-api-key
AZURE_SEARCH_INDEX_NAME=medical-knowledge-base

# DICOM/PACS Configuration (Optional)
PACS_AE_TITLE=ORTHANC
PACS_HOST=localhost
PACS_PORT=4242

# Application Insights (Optional - for observability)
APPLICATIONINSIGHTS_CONNECTION_STRING=InstrumentationKey=your-key

# Logging Configuration
LOG_LEVEL=INFO
ENABLE_TRACING=true
